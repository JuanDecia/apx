#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/juand/Desktop/proyectoapx/nivel-1/persistencia/node_modules/.pnpm/ava@6.4.0/node_modules/ava/entrypoints/node_modules:/mnt/c/Users/juand/Desktop/proyectoapx/nivel-1/persistencia/node_modules/.pnpm/ava@6.4.0/node_modules/ava/node_modules:/mnt/c/Users/juand/Desktop/proyectoapx/nivel-1/persistencia/node_modules/.pnpm/ava@6.4.0/node_modules:/mnt/c/Users/juand/Desktop/proyectoapx/nivel-1/persistencia/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/juand/Desktop/proyectoapx/nivel-1/persistencia/node_modules/.pnpm/ava@6.4.0/node_modules/ava/entrypoints/node_modules:/mnt/c/Users/juand/Desktop/proyectoapx/nivel-1/persistencia/node_modules/.pnpm/ava@6.4.0/node_modules/ava/node_modules:/mnt/c/Users/juand/Desktop/proyectoapx/nivel-1/persistencia/node_modules/.pnpm/ava@6.4.0/node_modules:/mnt/c/Users/juand/Desktop/proyectoapx/nivel-1/persistencia/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/ava@6.4.0/node_modules/ava/entrypoints/cli.mjs" "$@"
else
  exec node  "$basedir/../.pnpm/ava@6.4.0/node_modules/ava/entrypoints/cli.mjs" "$@"
fi
